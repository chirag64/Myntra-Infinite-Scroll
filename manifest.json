{
  "manifest_version": 2,
  "name": "Myntra Infinite Scroll",
  "version": "1.0",
  "author": "Chirag Bhatia",
  "content_scripts": [
    {
      "matches": ["*://*.myntra.com/*"],
      "run_at": "document_idle",
      "all_frames": true,
      "js": ["content.js"]
    }
  ],
  "icons": {
    "128": "icon.png"
  }
}